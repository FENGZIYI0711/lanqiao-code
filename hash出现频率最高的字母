#include <iostream>
#include <cstring>
using namespace std;
int main(){
	int n;
	cin>>n;
	while(n--){
		string s;
		cin>>s;
		int count[26]={0};
		for (int i=0;i<s.size();i++){
			count[s[i]-'a']++;
		}
		int flag=0;
		char result;
		for (int i=0;i<26;i++){
			if(count[i]>flag){
				flag=count[i];
				result=s[i];//result=i+'a';
			}
		}
		cout<<result<<endl;
	}
}
