#include <iostream>
#include <string>
using namespace std;
int main(){
    string s;
    while(getline(cin,s)){
       float sum=0;
       int count=0;
       float score=0;
       int flag=1;
        for(int i=0;i<s.size();i++){
            if(s[i]=='A'){
                sum+=4;
                count++;
            }else if(s[i]=='B'){
                sum+=3;
                count++;
            }else if(s[i]=='C'){
                sum+=2;
                count++;
            }else if(s[i]=='D'){
                sum+=1;
                count++;
            }else if((s[i]=='F')){
                sum+=0;
                count++;
            }else if((s[i]==' ')){
                continue;
			}else {
            	flag=0;
                cout<<"Unknown"<<endl;
                break;
            }
        }if(flag){
        printf("%.2f\n",score=sum/count);}
    }
    return 0;
}
